{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://com.aws.glue.schema.registry/SalesforceAudit.schema.json",
    "title": "SalesforceAudit",
    "description": "Schema for Salesforce audit events",
    "type": "object",
    "properties": {
      "eventId": {
        "type": "string",
        "description": "Unique identifier for the audit event"
      },
      "eventName": {
        "type": "string",
        "description": "Name of the audit event"
      },
      "timestamp": {
        "type": "integer",
        "description": "Timestamp of the event in milliseconds since epoch",
        "examples": [1698943200000]
      },
      "eventDetails": {
        "type": "string",
        "description": "Detailed information about the audit event"
      }
    },
    "required": ["eventId", "eventName", "timestamp", "eventDetails"],
    "additionalProperties": false
  }
  